{
  "version": 2,
  "name": "bandismartcards-backend",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "api/**"
    }
  }
}